{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Link", 
          "value": "<https://arrilan.com/$1>; rel=\"canonical\""
        }
      ]
    }
  ]
}

{
  "headers": [ /* your existing canonical config */ ],
  "rewrites": [
    {
      "source": "/api/make",
      "destination": "${MAKE_WEBHOOK_URL}"
    }
  ]
}
